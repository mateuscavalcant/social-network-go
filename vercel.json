{
    "version": 2,
    "builds": [
      { "src": "main.go", "use": "@vercel/go" }
    ],
    "routes": [
      { "src": "/public/(.*)", "dest": "/public/$1" },
      { "src": "/(.*)", "dest": "/main.go" }
    ],
    "functions": {
      "api/main.go": {
        "runtime": "go1.17"
      }
    }
  }
  